        android:supportsRtl="true"
        android:theme="@style/Theme.RemainingAssignments">
        <activity android:name=".RecyclerViewSecondActivity"></activity>
        <activity android:name=".RecyclerViewActivity" />
        <activity android:name=".ImageViews" />
